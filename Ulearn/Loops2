using System;

namespace Loops2
{
    class Program
    {
        static void Main()
        {
            int n = int.Parse(Console.ReadLine());
            int result = 0;
            for (int i = 1; i < 10; i++)
                for (int j = 0; j < 10; j++)
                    for (int k = 0; k < 10; k++)
                        if (i + j + k == n) result++;
            Console.WriteLine(result);
            Console.ReadKey();
        }
    }
}
